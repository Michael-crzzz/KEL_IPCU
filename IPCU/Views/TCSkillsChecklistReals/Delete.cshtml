@model IPCU.Models.TCSkillsChecklistReal

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete</h1>

<h3>Are you sure you want to delete this?</h3>
<form asp-action="Delete">
    <div class="row">
        <div class="col-md-6">
            <div class="form-group">
                <label asp-for="Fname" class="control-label">First Name:</label>
                <input asp-for="Fname" class="form-control" disabled />
            </div>
        </div>
        <div class="col-md-6">
            <div class="form-group">
                <label asp-for="Mname" class="control-label">Middle Name:</label>
                <input asp-for="Mname" class="form-control" disabled />
            </div>
        </div>
        <div class="col-md-6">
            <div class="form-group">
                <label asp-for="Lname" class="control-label">Last Name:</label>
                <input asp-for="Lname" class="form-control" disabled />
            </div>
        </div>
        <div class="col-md-6">
            <div class="form-group">
                <label asp-for="EmployeeId" class="control-label">Employee Id:</label>
                <input asp-for="EmployeeId" class="form-control" disabled />
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-6">
            <div class="form-group">
                <label asp-for="Area" class="control-label">AREA:</label>
                <input asp-for="Area" class="form-control" disabled />
            </div>
        </div>
        <div class="col-md-6">
            <div class="form-group">
                <label asp-for="Date" class="control-label">DATE/TIME (mm/dd/yyyy):</label>
                <input asp-for="Date" class="form-control" type="datetime-local" disabled />
            </div>
        </div>
    </div>

    <div class="row mb-3">
        <div class="col-md">
            <div class="form-group">
                <label asp-for="ObserverName" class="control-label">OBSERVER (Name):</label>
                <input asp-for="ObserverName" class="form-control" disabled />
            </div>
        </div>
    </div>

    <!-- Section 1: PREPARATION AND SETUP -->
    <div class="card mb-3">
        <div class="card-header bg-primary text-white">
            <h5>1. PREPARATION AND SETUP</h5>
        </div>
        <div class="card-body">
            <div class="form-check mb-2">
                <input asp-for="IsEquipmentAndCartPrepared" class="form-check-input" disabled />
                <label asp-for="IsEquipmentAndCartPrepared" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsCleaningSolutionPrepared" class="form-check-input" disabled />
                <label asp-for="IsCleaningSolutionPrepared" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsProperAttireAndPPEWorn" class="form-check-input" disabled />
                <label asp-for="IsProperAttireAndPPEWorn" class="form-check-label"></label>
            </div>
        </div>
    </div>

    <!-- Section 2: BASIC PROCEDURES -->
    <div class="card mb-3">
        <div class="card-header bg-primary text-white">
            <h5>2. BASIC PROCEDURES</h5>
        </div>
        <div class="card-body">
            <div class="form-check mb-2">
                <input asp-for="IsHandHygieneAndGlovesDone" class="form-check-input" disabled />
                <label asp-for="IsHandHygieneAndGlovesDone" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsSignageChecked" class="form-check-input" disabled />
                <label asp-for="IsSignageChecked" class="form-check-label"></label>
            </div>
        </div>
    </div>

    <!-- Section 3: PATIENT ROOM CLEANING PROCEDURES -->
    <div class="card mb-3">
        <div class="card-header bg-primary text-white">
            <h5>3. PATIENT ROOM CLEANING PROCEDURES</h5>
        </div>
        <div class="card-body">
            <div class="form-check mb-2">
                <input asp-for="IsSpillSoakedWithSolution" class="form-check-input" disabled />
                <label asp-for="IsSpillSoakedWithSolution" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsWallsCleaned" class="form-check-input" disabled />
                <label asp-for="IsWallsCleaned" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsDoorFrameWiped" class="form-check-input" disabled />
                <label asp-for="IsDoorFrameWiped" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsWindowSillAndWindowCleaned" class="form-check-input" disabled />
                <label asp-for="IsWindowSillAndWindowCleaned" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsHighTouchAreasWiped" class="form-check-input" disabled />
                <label asp-for="IsHighTouchAreasWiped" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsVerticalSurfacesWiped" class="form-check-input" disabled />
                <label asp-for="IsVerticalSurfacesWiped" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsLooseDebrisPickedUp" class="form-check-input" disabled />
                <label asp-for="IsLooseDebrisPickedUp" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsRoomFloorMopped" class="form-check-input" disabled />
                <label asp-for="IsRoomFloorMopped" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsUsedClothsDisposed" class="form-check-input" disabled />
                <label asp-for="IsUsedClothsDisposed" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsWasteContainersEmptied" class="form-check-input" disabled />
                <label asp-for="IsWasteContainersEmptied" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsInfectiousWasteRemoved" class="form-check-input" disabled />
                <label asp-for="IsInfectiousWasteRemoved" class="form-check-label"></label>
            </div>
        </div>
    </div>

    <!-- Section 4: BATHROOM CLEANING -->
    <div class="card mb-3">
        <div class="card-header bg-primary text-white">
            <h5>4. BATHROOM CLEANING</h5>
        </div>
        <div class="card-body">
            <div class="form-check mb-2">
                <input asp-for="IsMirrorCleaned" class="form-check-input" disabled />
                <label asp-for="IsMirrorCleaned" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsSinkAreaCleaned" class="form-check-input" disabled />
                <label asp-for="IsSinkAreaCleaned" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsFaucetAndHandlesCleaned" class="form-check-input" disabled />
                <label asp-for="IsFaucetAndHandlesCleaned" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsToiletAndFlushHandlesCleaned" class="form-check-input" disabled />
                <label asp-for="IsToiletAndFlushHandlesCleaned" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsOtherBathroomSurfacesCleaned" class="form-check-input" disabled />
                <label asp-for="IsOtherBathroomSurfacesCleaned" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsBathroomFloorScrubbed" class="form-check-input" disabled />
                <label asp-for="IsBathroomFloorScrubbed" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsColorCodedWasteEmptied" class="form-check-input" disabled />
                <label asp-for="IsColorCodedWasteEmptied" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsPPERemoved" class="form-check-input" disabled />
                <label asp-for="IsPPERemoved" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsHandHygieneAfterPPE" class="form-check-input" disabled />
                <label asp-for="IsHandHygieneAfterPPE" class="form-check-label"></label>
            </div>
            <div class="form-check mb-2">
                <input asp-for="IsGlovesRemovedAndHandHygieneDone" class="form-check-input" disabled />
                <label asp-for="IsGlovesRemovedAndHandHygieneDone" class="form-check-label"></label>
            </div>
        </div>
    </div>

    <!-- VALIDATION WITH FLUORESCENCE MARKERS -->
    <div class="card mb-3">
        <div class="card-header bg-primary text-white">
            <h5>VALIDATION WITH FLUORESCENCE MARKERS</h5>
        </div>
        <div class="card-body">
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label asp-for="PreCleaningItems" class="control-label">Pre-cleaning areas/items</label>
                        <textarea asp-for="PreCleaningItems" class="form-control" rows="3" disabled></textarea>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label asp-for="PostCleaningItems" class="control-label">Post-cleaning</label>
                        <textarea asp-for="PostCleaningItems" class="form-control" rows="3" disabled></textarea>
                    </div>
                </div>
            </div>
            <div class="form-group mt-3">
                <label asp-for="RecommendationsOrActions" class="control-label">Recommendations / Actions Taken</label>
                <textarea asp-for="RecommendationsOrActions" class="form-control" rows="3" disabled></textarea>
            </div>
        </div>
    </div>

    <div class="form-group">
        <label asp-for="UnitAreaStaffSignature" class="control-label">Unit Area Staff Signature</label>
        <input asp-for="UnitAreaStaffSignature" class="form-control" disabled />
    </div>

    <div class="form-group mb-3">
        <label asp-for="DateOfObservation" class="control-label">Date of Observation</label>
        <input asp-for="DateOfObservation" class="form-control" type="date" disabled />
    </div>

    <input type="hidden" asp-for="Id" />
    <input type="submit" value="Delete" class="btn btn-danger" /> |
    <a asp-action="Index">Back to List</a>

</form>





